<!DOCTYPE html>
<html data-bs-theme="dark">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - James Sampica</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    </environment>
    <environment include="Production">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    </environment>
</head>
<body hx-boost="true">
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm mb-3">
            <div class="container">
                <a class="navbar-brand fs-4" asp-area="" asp-page="/Index">James Sampica</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
            </div>
        </nav> 
    </header>
    <div class="container">
        <main role="main">
            @RenderBody()
        </main>
        <footer class="my-4">
            <h5>Get in touch...</h5>
            <p>jtsampica@gmail.com</p>
            <a target="_blank" href="https://github.com/jamesSampica">Github</a>
            <br>
            <a target="_blank" href="https://stackoverflow.com/users/1950321/james-sampica">StackOverflow</a>
            <br>
            <a target="_blank" href="https://www.linkedin.com/in/jamessampica/">LinkedIn</a>
            <br>
            <a target="_blank" href="https://twitter.com/JamesSampica">Twitter</a>
        </footer>
    </div>
</body>
</html>
